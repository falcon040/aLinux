Xft.dpi:                            96
!Xft.antialias:              true
!Xft.hinting:            full
!Xft.hintstyle:              hintslight
!Xft.rgba:                           rgb
Xcursor.theme:          Neutral_Plus_Inverted
Xcursor.size:           10
 
URxvt.termName:                 rxvt-unicode
 
URxvt.imLocale:                 de_DE.utf8
URxvt.scrollBar:                false
URxvt.saveLines:                32767
URxvt.cursorBlink:          true
URxvt.cursorColor:          #FFFFFF
 
URxvt.fading:                  30
URxvt.fadeColor:           black
URxvt.shading:                  20
URxvt.tintColor:        #000000
URxvt.depth:                    24
 
!URxvt.InternalBorder:  .1
!URxvt.externalBorder:  .2
!URxvt.borderColor:     #333333
 
URxvt.visualBell:               true
 
!! urlLauncher settings
URxvt.perl-ext-common : default,matcher
URxvt.urlLauncher     : firefox
URxvt.matcher.button  : 1

!URxvt.perl-lib:           /usr/lib/urxvt/perl/
!URxvt.perl-ext-common:    default,matcher
!URxvt.urlLauncher:        /usr/bin/firefox
!URxvt.matcher.button:     1
!URxvt.matcher.pattern.1:  \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
 
!! Fonts
URxvt.font:     xft:bitstream vera sans mono:size=12:antialias=true
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=12:antialias=true

!URxvt.font: 		xft:monaco:size=10
!URxvt.boldFont: 	xft:monaco:bold:size=10
!URxvt.letterSpace: -2

!URxvt.font:  -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso8859-1
!URxvt.font:                 xft:Monaco:pixelsize=12:antialias=true:hinting=true
!URxvt.boldFont:                 xft:Monaco:pixelsize=9:antialias=true:bold:hinting=true
!URxvt.italicFont:               xft:Monaco:pixelsize=9:antialias=true:italic:autohint=true:hinting=true
!URxvt.bolditalicFont:           xft:Monaco:pixelsize=9:antialias=true:bold:italic:autohint=true:hinting=true
!URxvt.font:                -*-Monaco-medium-r-*-*-9-120-*-*-*-*-*-*
!URxvt.boldFont:            -*-Monaco-medium-r-*-*-9-120-*-*-*-*-*-*
 
!! Colours
*background:                #202020
*foreground:                #B4B4B4
 
